  "scripts": {
    "start": "PORT=4000 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "rm ./node_modules/firebase/empty-import.d.ts",
    "link-shared": "tsc functions/src/feedfarm-shared/utils.ts && yarn add ./functions/src/feedfarm-shared",
    "delete-source-maps": "rm ./build/static/js/*.map && rm ./build/static/css/*.map",
    "deploy": "node find-dnd-flag.js && yarn link-shared && yarn build && firebase deploy -m \"`git describe --tags`\"",
    "deploy-hosting": "node find-dnd-flag.js && yarn link-shared && yarn build && yarn delete-source-maps && firebase deploy --only hosting -m \"`git describe --tags`\"",
    "deploy-functions": "rm -rf functions/lib/ && firebase deploy --only functions -m \"`git describe --tags`\"",
    "__rm-tsc": "rm -rf ./lib",
    "__build-tsc": "tsc --outDir lib --noEmit false --target ES5 --jsx preserve",
    "__i18n": "i18next-scanner",
    "extract-i18n": "yarn __rm-tsc && yarn __build-tsc && yarn __i18n",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "create-functions-declarations": "createFunctionsDeclarations --output ./src/firebase-functions"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "find-in-files": "^0.5.0",
    "i18next-scanner": "^2.10.3",
    "jest": "^24.9.0",
    "jest-puppeteer": "^4.3.0",
    "puppeteer": "^2.0.0"
  }
}
